// diagnostico alex.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main ()
{
	int n, suma, i;

	cout<< "Ingrese el numero: ";
	cin>> n;

	suma=0;

	for(i=1; i<=n; i=i+2)
	{
	suma=suma+i;
	}

	cout<<"La sumatoria es: ";
	cout<<suma;

	getch();
}
