#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main()
{
  int i, n, suma;
do
				{
					cout<<"\nIngrese la cantidad: \n\n";
					cin>>n;
				}
				while (n<0);
				{
					suma=0;
					for(i=1;i<=n;i=i+2)
					suma=suma+i;
				}			
				cout<<"\nla sumatoria es igual a: \n\n";
				cout<<suma;
					getch();
